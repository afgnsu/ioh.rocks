<%= render "live_nav" %>

<table class="table table-hover">
	<thead>
		<tr>
				<th>名字</th>
				<th>頻道</th>
				<th>Live Host</th>
				<th>學校</th>
				<th>科系</th>
				<th>直播地點</th>
				<th>影音同意書</th>
				<th>Q&A 連結</th>
				<th>文件命名</th>
				<th>直播命名</th>
		</tr>
	</thead>
	<tbody>
		<% @lives.each do |live| %>
			<tr>
				<td><%= live.name %></td>
				<td><%= live.chennal %></td>
				<td><%= live.live_host %></td>
				<td><%= live.live_school.name if live.live_school %></td>
				<td><%= live.live_department.name %></td>
				<td><%= live.location %></td>
				<th><%= live.audio_agree %></th>
				<th><%= live.qa_link %></th>
				<th><%= live.doc_naming %></th>
				<th><%= live.stream_naming %></th>
			</tr>
		<% end %>
	</tbody>
</table>