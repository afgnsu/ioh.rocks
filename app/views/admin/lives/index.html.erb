<table class="table table-hover">
	<thead>
		<tr>
				<th>名字</th>
				<th>gmail</th>
				<th>學校</th>
				<th>科系</th>
				<th>fb</th>
				<th>電話</th>
				<th>上次直播</th>
				<th>直播地點</th>
				<th>feedback</th>
		</tr>
	</thead>
	<tbody>
		<% @lives.each do |live| %>
			<tr>
				<td><%= live.name %></td>
				<td><%= live.gmail %></td>
				<td><%= live.live_school.name if live.live_school %></td>
				<td><%= live.live_department.name %></td>
				<td><%= live.fb_url %></td>
				<td><%= live.phone %></td>
				<td><%= live.stream_201602 %></td>
				<td><%= live.location %></td>
				<td><%= live.feedback %></td>
			</tr>
		<% end %>
	</tbody>
</table>
